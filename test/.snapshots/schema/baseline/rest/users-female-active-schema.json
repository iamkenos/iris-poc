{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "minItems": 1,
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "number"
      },
      "name": {
        "type": "string"
      },
      "email": {
        "type": "string"
      },
      "gender": {
        "type": "string",
        "enum": ["female"]
      },
      "status": {
        "type": "string",
        "enum": ["active"]
      }
    },
    "required": ["id", "name", "email", "gender", "status"]
  }
}
